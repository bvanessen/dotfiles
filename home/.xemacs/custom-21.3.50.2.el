(custom-set-variables
  ;; custom-set-variables was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(font-lock-mode t t (font-lock))
 '(global-font-lock-mode t nil (font-core))
 '(line-number-mode t)
 '(paren-mode (quote sexp) nil (paren))
 '(show-paren-mode t nil (paren))
 '(specman-auto-newline nil)
 '(standard-indent 2)
 '(tab-stop-list (quote (2 4 6 8 10 12 14 16 24 32 40 48 56 64 72 80 88 96 104 112 120)))
 '(transient-mark-mode t)
 '(verilog-auto-newline nil)
 '(verilog-compiler "verilog ")
 '(verilog-indent-declaration-macros t)
 '(verilog-indent-lists nil)
 '(verilog-tab-to-comment nil))
(custom-set-faces
  ;; custom-set-faces was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(default ((t (:stipple nil :background "white" :foreground "black" :inverse-video nil :box nil :strike-through nil :overline nil :underline nil :slant normal :weight normal :height 96 :width normal :family "apple-monaco")))))
